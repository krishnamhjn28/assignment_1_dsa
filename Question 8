#include <iostream>
using namespace std;
void bubbleSort(int arr[], int n) {
    for (int i = 0; i < n - 1; ++i) {
        for (int j = 0; j < n - i - 1; ++j) {
            if (arr[j] > arr[j + 1]) {
                // Swap arr[j] and arr[j+1]
                int temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }
}
int countDistinct(int arr[], int n) {
    bubbleSort(arr, n);

    int count = 1; // First element is always distinct
    for (int i = 1; i < n; ++i) {
        if (arr[i] != arr[i - 1]) {
            count++;
        }
    }

    return count;
}
int main() {
    int n;
    cout << "Enter number of elements: ";
    cin >> n;

    int arr[100]; 
    cout << "Enter array elements:\n";
    for (int i = 0; i < n; ++i) {
        cin >> arr[i];
    }
    int result = countDistinct(arr, n);
    cout << "Number of distinct elements: " << result << endl;
}
